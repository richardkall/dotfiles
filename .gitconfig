[alias]
  aa = add --all
  br = branch
  ca = commit --amend
  ci = commit -v
  co = checkout
  lg = log --oneline --decorate --graph --date=relative
  st = status
  changelog = "!f() { git log --pretty=format:%s $1..HEAD; }; f"
[core]
  autocrlf = input
  excludesfile = ~/.gitignore
[credential]
  helper = osxkeychain
[include]
  path = ~/.gitconfig.local
[init]
  defaultBranch = main
[pull]
  rebase = true
