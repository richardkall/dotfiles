[alias]
  aa = add --all
  ap = add --patch
  ca = commit --amend
  ci = commit -v
  co = checkout
  lg = log --oneline --decorate --graph --date=relative
  st = status
  w = whatchanged
[color]
  ui = auto
[core]
  autocrlf = input
  excludesfile = ~/.gitignore
[credential]
  helper = osxkeychain
[include]
  path = ~/.gitconfig.local
[merge]
  ff = only
[push]
  default = current
